# Update these with your Supabase details from your project settings > API
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your-publishable-or-anon-key

# Supabase Admin (for audit logging and admin operations)
# WARNING: Never expose this key to the client. Keep it server-side only.

SUPABASE_SECRET_KEY=your-secret-key


# Email configuration (optional - for contact form and notifications)
# Get your API key from https://resend.com/api-keys
RESEND_API_KEY=your-resend-api-key
CONTACT_EMAIL=your-contact-email@example.com
RESEND_FROM_EMAIL=noreply@yourdomain.com
